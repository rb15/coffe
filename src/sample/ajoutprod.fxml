<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="179.0" prefWidth="325.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ajoutprodControlller">
    <children>
        <HBox prefHeight="102.0" prefWidth="268.0">
            <children>
                <VBox prefHeight="72.0" prefWidth="75.0" spacing="32.0">
                    <children>
                        <Label alignment="CENTER" prefHeight="16.0" prefWidth="76.0" text="Categorie" />
                        <Label alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="16.0" prefWidth="72.0" text="produit" />
                  <Label alignment="CENTER" layoutX="10.0" layoutY="58.0" prefHeight="16.0" prefWidth="72.0" text="contite" />
                  <Label alignment="CENTER" layoutX="10.0" layoutY="106.0" prefHeight="16.0" prefWidth="72.0" text="prix" />
                  <Label alignment="CENTER" layoutX="10.0" layoutY="154.0" prefHeight="16.0" prefWidth="72.0" text="prix vente" />
                    </children>
                </VBox>
                <VBox prefHeight="205.0" prefWidth="176.0" spacing="20.0">
                    <padding>
                        <Insets left="20.0" />
                    </padding>
                    <children>
                        <ComboBox fx:id="cate" prefWidth="150.0" />
                        <TextField fx:id="prod" />
                  <TextField fx:id="cont" layoutX="30.0" layoutY="56.0" />
                  <TextField fx:id="prix" layoutX="30.0" layoutY="56.0" />
                  <TextField fx:id="prix_v" layoutX="30.0" layoutY="102.0" />
                    </children>
                </VBox>
            </children>
            <padding>
                <Insets bottom="20.0" left="40.0" right="20.0" top="20.0" />
            </padding>
        </HBox>
        <HBox prefHeight="44.0" prefWidth="325.0" spacing="30.0">
            <children>
                <Button fx:id="ajout" mnemonicParsing="false" onAction="#ajout" text="Ajouter" />
                <Button fx:id="close" mnemonicParsing="false" onAction="#close" text="Annuler" />
            </children>
            <padding>
                <Insets bottom="20.0" left="90.0" right="20.0" top="10.0" />
            </padding>
        </HBox>
    </children>
</VBox>
