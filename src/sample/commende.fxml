<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.commendecontroller">
   <children>
      <TreeView fx:id="tree" onMouseClicked="#click" prefHeight="200.0" prefWidth="200.0" />
      <Button mnemonicParsing="false" onAction="#ajouter" text="Button" />
      <VBox>
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox fx:id="prod" prefHeight="200.0" prefWidth="100.0" />
                  <VBox fx:id="prod1" layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="100.0" />
                  <Button mnemonicParsing="false" onAction="#supp" text="Button" />
               </children>
            </HBox>
            <Label prefHeight="16.0" prefWidth="195.0" text="-----------------------------" />
            <HBox layoutX="10.0" layoutY="10.0" prefHeight="83.0" prefWidth="200.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Totale" />
                     </children></VBox>
                  <VBox layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label fx:id="total" text="0.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</HBox>
